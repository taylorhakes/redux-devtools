{
  "name": "Redux Developer Tools",
  "description": "Adds Redux debugging tools to the Chrome Developer Tools.",
  "version": "0.1.0",
  "manifest_version": 2,
  "devtools_page": "index.html",
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/contentScript.js"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [ "src/pageScript.js"]
}